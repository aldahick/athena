{
  "name": "@athenajs/utils",
  "version": "0.0.13",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "postversion": "bash ../../scripts/hooks/postversion.sh utils",
    "preversion": "pnpm build",
    "test": "c8 node --enable-source-maps --test dist"
  },
  "repository": {
    "url": "git+https://github.com/aldahick/athena"
  },
  "c8": {
    "100": true,
    "all": true,
    "exclude-after-remap": true,
    "exclude": [
      "src/index.ts",
      "src/*.test.ts",
      "dist/*"
    ]
  },
  "dependencies": {
    "graphql": "^16.7.1"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "c8": "^7.14.0"
  }
}
