{
  "name": "@athenajs/utils",
  "version": "0.0.11",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "postversion": "bash ../../scripts/hooks/postversion.sh utils",
    "preversion": "pnpm build",
    "test": "c8 node --enable-source-maps --test dist"
  },
  "c8": {
    "100": true,
    "all": true,
    "exclude-after-remap": true,
    "exclude": [
      "src/index.ts",
      "src/*.test.ts",
      "dist/*"
    ]
  },
  "dependencies": {
    "graphql": "16.6.0"
  },
  "devDependencies": {
    "@types/node": "20.1.2",
    "c8": "7.14.0"
  }
}
