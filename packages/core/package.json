{
  "name": "@athenajs/core",
  "version": "1.0.1",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:full": "cd ../utils && pnpm build && cd ../core && pnpm build",
    "build": "tsc",
    "lint": "eslint src",
    "postversion": "bash ../../scripts/hooks/postversion.sh core",
    "preversion": "pnpm build",
    "test": "c8 node --enable-source-maps --trace-warnings --test dist"
  },
  "engines": {
    "node": ">=16.13.2"
  },
  "repository": {
    "url": "git+https://github.com/aldahick/athena"
  },
  "c8": {
    "100": true,
    "all": true,
    "exclude-after-remap": true,
    "exclude": [
      "src/graphql.ts",
      "src/index.ts",
      "src/*/index.ts",
      "src/*.test.ts",
      "src/*/*.test.ts",
      "dist/*"
    ]
  },
  "dependencies": {
    "@apollo/server": "^4.9.4",
    "@as-integrations/fastify": "^2.1.1",
    "@athenajs/utils": "workspace:^",
    "@fastify/cors": "^8.4.0",
    "@fastify/multipart": "^8.0.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.23.2",
    "graphql": "^16.8.1",
    "graphql-resolve-batch": "^1.0.3",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.8.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/graphql-resolve-batch": "^1.1.7",
    "@types/node": "^20.8.3",
    "c8": "^8.0.1"
  }
}
