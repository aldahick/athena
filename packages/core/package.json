{
  "name": "@athenajs/core",
  "version": "0.0.13",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:full": "cd ../utils && pnpm build && cd ../core && pnpm build",
    "build": "tsc",
    "lint": "eslint src",
    "postversion": "bash ../../scripts/hooks/postversion.sh core",
    "preversion": "pnpm build",
    "test": "c8 node --enable-source-maps --test dist"
  },
  "repository": {
    "url": "git+https://github.com/aldahick/athena"
  },
  "c8": {
    "100": true,
    "all": true,
    "exclude-after-remap": true,
    "exclude": [
      "src/graphql.ts",
      "src/index.ts",
      "src/*/index.ts",
      "src/*.test.ts",
      "src/*/*.test.ts",
      "dist/*"
    ]
  },
  "dependencies": {
    "@aldahick/tsyringe": "4.7.1",
    "@apollo/server": "4.7.1",
    "@as-integrations/fastify": "1.3.2",
    "@athenajs/utils": "workspace:^",
    "@fastify/cors": "8.3.0",
    "dotenv": "16.0.3",
    "fastify": "4.17.0",
    "graphql": "16.6.0",
    "graphql-resolve-batch": "1.0.3",
    "reflect-metadata": "0.1.13",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@types/graphql-resolve-batch": "1.1.6",
    "@types/node": "20.1.2",
    "c8": "7.14.0"
  }
}
