{
  "name": "@athenajs/core",
  "version": "1.1.2",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:full": "cd ../utils && pnpm build && cd ../core && pnpm build",
    "build": "tsc",
    "lint": "biome check src",
    "postversion": "bash ../../scripts/hooks/postversion.sh core",
    "preversion": "pnpm build",
    "test": "c8 node --enable-source-maps --trace-warnings --test dist"
  },
  "engines": {
    "node": "^20.10"
  },
  "repository": {
    "url": "git+https://github.com/aldahick/athena"
  },
  "c8": {
    "100": true,
    "all": true,
    "exclude-after-remap": true,
    "exclude": [
      "src/graphql.ts",
      "src/index.ts",
      "src/*/index.ts",
      "src/*.test.ts",
      "src/*/*.test.ts",
      "dist/*"
    ]
  },
  "dependencies": {
    "@apollo/server": "^4.10.2",
    "@as-integrations/fastify": "^2.1.1",
    "@athenajs/utils": "workspace:^",
    "@fastify/cors": "^9.0.1",
    "@fastify/multipart": "^8.2.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "graphql": "^16.8.1",
    "graphql-resolve-batch": "^1.0.3",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.8.0",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/graphql-resolve-batch": "^1.1.11"
  }
}
