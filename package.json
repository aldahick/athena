{
  "name": "@athenajs/core",
  "version": "1.0.0-beta5",
  "description": "Framework for GraphQL and REST APIs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint": "eslint --ext .ts src",
    "format": "prettier --config .prettierrc --write src",
    "test": "jest",
    "test:dev": "tsc-watch --onSuccess \"yarn test\"",
    "demo": "node dist/demo/index.js",
    "demo:dev": "tsc-watch --onSuccess \"node dist/demo/index.js\" | bunyan",
    "demo:generate": "graphql-codegen --config src/demo/graphql/codegen.yml",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aldahick/athena.git"
  },
  "author": "Alex Hicks <alex@alexhicks.net> (https://alexhicks.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aldahick/athena/issues"
  },
  "homepage": "https://github.com/aldahick/athena#readme",
  "dependencies": {
    "@typegoose/typegoose": "9.12.0",
    "accesscontrol": "2.2.1",
    "apollo-server-express": "3.10.2",
    "bunyan": "1.8.15",
    "cors": "2.8.5",
    "dotenv": "16.0.2",
    "express": "4.18.1",
    "fs-extra": "10.1.0",
    "graphql": "16.6.0",
    "ioredis": "5.2.3",
    "joi": "17.6.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "mongoose": "6.6.1",
    "pg": "8.8.0",
    "randomstring": "1.2.2",
    "recursive-readdir": "2.2.2",
    "reflect-metadata": "0.1.13",
    "socket.io": "4.5.2",
    "source-map-support": "0.5.21",
    "tsyringe": "4.7.0",
    "typeorm": "0.3.10"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.12.1",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@types/bunyan": "1.8.8",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.14",
    "@types/fs-extra": "9.0.13",
    "@types/ioredis": "4.28.10",
    "@types/jest": "29.0.3",
    "@types/jsonwebtoken": "8.5.9",
    "@types/lodash": "4.14.185",
    "@types/node": "18.7.20",
    "@types/randomstring": "1.1.8",
    "@types/recursive-readdir": "2.2.1",
    "@types/source-map-support": "0.5.6",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "eslint": "8.24.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "jest": "29.0.3",
    "prettier": "2.7.1",
    "tsc-watch": "5.0.3",
    "typescript": "4.8.3"
  }
}
