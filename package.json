    "url": "https://github.com/aldahick/athena/issues"
  },
  "homepage": "https://github.com/aldahick/athena#readme",
  "dependencies": {
    "@typegoose/typegoose": "^7.6.0",
    "@types/express": "^4.17.9",
    "@types/joi": "^14.3.4",
    "@types/mongoose": "^5.10.3",
    "@types/socket.io": "^2.1.12",
    "accesscontrol": "^2.2.1",
    "apollo-server-express": "^2.19.0",
    "bunyan": "^1.8.14",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
