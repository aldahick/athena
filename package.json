{
  "name": "@athenajs/core",
  "version": "0.6.2",
  "description": "Framework for GraphQL and REST APIs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "build:watch": "tsc -w -p .",
    "lint": "eslint --ext .ts .",
    "test": "mocha dist/**/*.spec.js",
    "test:dev": "tsc-watch --onSuccess \"yarn test\"",
    "demo": "node dist/demo/index.js",
    "demo:dev": "tsc-watch --onSuccess \"node dist/demo/index.js\" | bunyan",
    "demo:generate": "graphql-codegen --config src/demo/graphql/codegen.yml",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aldahick/athena.git"
  },
  "author": "Alex Hicks <alex@alexhicks.net> (https://alexhicks.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aldahick/athena/issues"
  },
  "homepage": "https://github.com/aldahick/athena#readme",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@typegoose/typegoose": "^7.3.0",
    "@types/express": "^4.17.7",
    "@types/hapi__joi": "^17.1.4",
    "@types/socket.io": "^2.1.8",
    "accesscontrol": "^2.2.1",
    "apollo-server-express": "^2.15.1",
    "bunyan": "^1.8.14",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "graphql": "^15.3.0",
    "ioredis": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "mongoose": "^5.9.23",
    "pg": "^8.3.0",
    "randomstring": "^1.1.5",
    "recursive-readdir": "^2.2.2",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^2.3.0",
    "source-map-support": "^0.5.19",
    "tsyringe": "^4.3.0",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@aldahick/eslint-rules": "^0.8.0",
    "@graphql-codegen/cli": "^1.17.0",
